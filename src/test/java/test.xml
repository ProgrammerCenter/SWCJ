<?xml version="1.0" encoding="UTF-8" ?>
<SWCJ>
    <!--全局配置-->
    <config>
        <constructionSpace isAbsolute="true" workSpace="E:\临时文件"/>
        <timeout value="10000"/>
        <userAgent>
            <value>Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/97.0.4692.71
                Safari/537.36 Edg/97.0.1072.62
            </value>
            <value>User-Agent: Mozilla/4.0 (compatible; MSIE 7.0; Windows NT 5.1; TencentTraveler 4.0)</value>
            <value>User-Agent: Mozilla/4.0 (compatible; MSIE 7.0; Windows NT 5.1; 360SE)</value>
        </userAgent>
        <createTactics isCache="false"/>
        <chooseStrategy type="METHOD_NAME"/>
    </config>
    <!--具体的某个爬虫类
         id 获取的标识
        -->
    <swc>
        <id>downloader</id>
        <cookies>
        </cookies>
        <parentInterface>test.pojo</parentInterface>
        <url>
            <name>gethtml</name>
            <type>GET</type>
            <inPutName>key</inPutName>
            <path>https://s.pcmgr.qq.com/tapi/web/searchcgi.php?type=search&amp;callback=searchCallback&amp;keyword=#{key}&amp;page=1&amp;pernum=30&amp;more=0</path>
            <parseProgram>
                <isHtml>false</isHtml>
                <type>reg</type>
                <xml>
                    <reg>
                        <reg name="name" del="'&quot;SoftName&quot;:&quot;';'&quot;';">
                            "SoftName":".+?"
                        </reg>
                    </reg>
                </xml>
            </parseProgram>
        </url>
    </swc>
</SWCJ>